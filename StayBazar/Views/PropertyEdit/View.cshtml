@model List<CLayer.RatesAccommodationModel>
@{
    ViewBag.Title = "listview";
    int list = Model.Count;
    Layout = null;
}
@if (list > 0)
{
    <div class="row">
        <div class="col-md-12">
            <table  class="table table-condensed table-responsive table-custom">
                <thead>
                    <tr>
                        <th>DailyRate</th>
                        <th>GuestRate</th>
                        <th>LongTermRate</th>
                        <th>MonthlyRate</th>
                        <th>WeeklyRate</th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (CLayer.RatesAccommodationModel item in Model)
                    {
                        <tr>
                            <td>@item.RCDaily</td>
                            <td>@item.RCGuest</td>
                            <td>@item.RCLong</td>
                            <td>@item.RCMonthly</td>
                            <td>@item.RCWeekly</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
}
else
{
    <div class="row margin-top-10">
        <div class="col-md-4 col-md-offset-4 margin-top-10">
            <div class="alert alert-info">Coming Soon</div>
        </div>
    </div>
}
