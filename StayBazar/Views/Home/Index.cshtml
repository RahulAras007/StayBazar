@{
    /**/

    ViewBag.Title = "Home Page";
}
<style>
.slick-slide img
    {      
        padding: 10px;       

    }
    .slick-slide{
     width:150px;
    }
    section {
        padding: 30px 100px;
    }
</style>


<!--Landing Starts Here -->

<div id="landing-carousel" class="carousel slide" data-ride="carousel">
    <ul class="carousel-indicators">
        <li data-target="#landing-carousel" data-slide-to="0" class="active"></li>
        <li data-target="#landing-carousel" data-slide-to="1"></li>
        <li data-target="#landing-carousel" data-slide-to="2"></li>
    </ul>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="/img/Slider-1.jpg" alt="">
            <div class="carousel-caption">
                <h1>Business Travel and MICE under one roof!</h1>
            </div>
        </div>
        <div class="carousel-item">
            <img src="/img/Slider-2.jpg" alt="">
            <div class="carousel-caption">
                <h1>Better Choice. Best  Experience</h1>
            </div>
        </div>
        <div class="carousel-item">
            <img src="/img/Slider-3.jpg" alt="">
            <div class="carousel-caption">
                <h1>Impressive Service. Expressive Travel</h1>
            </div>
        </div>

        <img class="carousel-icon" src="img/landing-icon.png" alt="">
    </div>
    <a class="carousel-control-prev" href="#landing-carousel" data-slide="prev">
        <span class="carousel-control-prev-icon"></span>
    </a>
    <a class="carousel-control-next" href="#landing-carousel" data-slide="next">
        <span class="carousel-control-next-icon"></span>
    </a>
</div>

<!-- Landing Ends Here -->
<!-- Facts and Figures Starts Here -->

<section class="facts" id="facts">
    <h2 class="section-heading">FACTS & FIGURES</h2>
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-3">
                <h3><span class="count" data-animated="false">10000</span>+</h3>
                <p>Happy Guests</p>
            </div>
            <div class="col-md-3">
                <h3><span class="count" data-animated="false">300</span>+</h3>
                <p>Corporate Customers</p>
            </div>
            <div class="col-md-3">
                <h3><span class="count" data-animated="false">5000</span>+</h3>
                <p>Locations</p>
            </div>
            <div class="col-md-3">
                <h3><span class="count" data-animated="false">800000</span>+</h3>
                <p>Accommodation Options</p>
            </div>
        </div>
    </div>
</section>

<!-- Facts and Figures Ends Here -->
