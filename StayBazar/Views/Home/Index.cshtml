                <a href="~/Insights/ORGANISATION">
                    <img src='/img/insight-2.png' alt="">
                    <h3>ORGANISATION</h3>
                </a>
            </div>
            <div class="insight-container deg135" style="top:190px;left:320px;">
                <a href="~/Insights/TRAVELDESK">
                    <img src='/img/insight-3.png' alt="">
                    <h3>TRAVEL DESK</h3>
                </a>
            </div>
            <div class="insight-container deg180" style="top:250px;left:400px;">
                <a href="~/Insights/FINANCEACCOUNTS">
                    <img src='/img/insight-4.png' alt="">
