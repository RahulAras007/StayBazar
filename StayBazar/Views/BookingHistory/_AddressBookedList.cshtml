@model IEnumerable<CLayer.Address>
@{
    Layout = null;
    }
    @if (Model != null)
{
    foreach(CLayer.Address usr1 in Model)
    {       
      <text>
                Buyer :  <br />    
                First Name:  @usr1.Firstname<br />
                Last Name: @usr1.Lastname<br />
                Address:@usr1.AddressText<br />
                City:@usr1.City <br />
                State: @usr1.StateName  <br />
                Country:@usr1.Country<br />
                Phone:@usr1.Phone , @usr1.Mobile<br />
                Email:@usr1.Email                 
     </text>
        
            }
        }
