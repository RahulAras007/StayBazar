                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true)
                @Html.HiddenFor(m => m.Website)
                <div class="form-group">
                    <label class="control-label col-sm-4" for="Name">Name:</label>
                    <div class="col-sm-8">
                        @Html.TextBoxFor(m => m.Name, new { @class = "form-control", @placeholder = "Enter Name" })
                        @*<input type="text" class="form-control" name="Name" id="Name" placeholder="Enter Name">*@
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-sm-4" for="ContactNo">Contact No:</label>
                    <div class="col-sm-8">
                        @Html.TextBoxFor(m => m.ContactNo, new { @class = "form-control", @placeholder = "Enter Contact No" })
                        @*<input type="text" name="ContactNo" class="form-control" id="ContactNo" placeholder="Enter Contact No">*@
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-sm-4" for="Email">Email:</label>
                    <div class="col-sm-8">
                        @Html.TextBoxFor(m => m.Email, new { @class = "form-control", @type = "email", @placeholder = "Enter Email" })
                        @*<input type="email" class="form-control" name="Email" id="Email" placeholder="Enter Email">*@
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-sm-4" for="EventName">Meet Name:</label>
                    <div class="col-sm-8">
                        @Html.TextBoxFor(m => m.EventName, new { @class = "form-control", @placeholder = "Enter Meet Name" })
                        @* <input type="text" class="form-control" name="EventName" id="EventName" placeholder="Enter Event Name">*@
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-sm-4" for="EventLocation">Meet Location:</label>
                    <div class="col-sm-8">
                        @Html.TextBoxFor(m => m.EventLocation, new { @class = "form-control", @placeholder = "Enter Meet Location" })
                        @*  <input type="text" class="form-control" name="EventLocation" id="EventLocation" placeholder="Enter Event Location">*@
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-sm-4">No Of Travellers:</label>
                    <div class="col-sm-2">
                        <label>Adults </label>
                    </div>
                    <div class="col-sm-2">
                        @Html.TextBoxFor(m => m.NoOfAdult, new { @class = "form-control", @placeholder = "Enter Adult" })
                        @*   <input type="text" class="form-control"  name="NoOfAdult" id="NoOfAdult">*@
                    </div>
                    <div class="col-sm-2">
                        <label>Children </label>
                    </div>
                    <div class="col-sm-2">
                        @Html.TextBoxFor(m => m.NoOfChild, new { @class = "form-control", @placeholder = "Enter Child" })
                        @* <input type="text" class="form-control" name="NoOfChild" id="NoOfChild">*@
                    </div>

                </div>


                <div class="form-group">
                    <label class="control-label col-sm-4" for="NoOfRooms">No Of Rooms Required:</label>
                    <div class="col-sm-8">
                        @Html.TextBoxFor(m => m.NoOfRooms, new { @class = "form-control", @placeholder = "Enter Rooms" })
                        @*  <input type="text" class="form-control" name="EventLocation" id="EventLocation" placeholder="Enter Event Location">*@
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-sm-4" for="CheckIn">Check In Date:</label>
                    <div class="col-sm-8">
                        @Html.TextBoxFor(m => m.CheckIn, new { @class = "form-control input-sm", @placeholder = "Enter Check In Date" })
                        @*  <input type="text" id="CheckIn" name="CheckIn" class="form-control input-sm" placeholder="Check In" onchange="changecheckin()">*@
                        @*  @Html.ValidationMessageFor(m => m.CheckIn)*@
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-sm-4" for="CheckOut">Check Out Date:</label>
                    <div class="col-sm-8">
                        @Html.TextBoxFor(m => m.CheckOut, new { @class = "form-control input-sm", @placeholder = "Enter Check Out Date" })
                        @* <input type="text" id="CheckOut" name="CheckOut" class="form-control input-sm" onchange="changecheckout()" placeholder="Check Out">*@
                        @*@Html.ValidationMessageFor(m => m.CheckOut)*@
                    </div>
                </div>
                <div class="col-md-offset-10">
                    <div class="submitctr">
                        <input type="submit" id="btnSave" value="Submit" class="btn btn-primary" />
                    </div>
                </div>
                <br />
            }
        </div>
        <div class="col-md-6 sidesplash">
            <img src="/images/imac.jpg" />
        </div>
    </div>

