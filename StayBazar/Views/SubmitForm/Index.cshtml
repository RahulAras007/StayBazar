@model StayBazar.Models.FormSubmitModel
@{
    ViewBag.Title = "Book Your Accommodation";
}


<div class="container">
    <div class="row margin-bot-10">
        <div class="col-md-12">
            <h2 class="form-header">
                @ViewBag.Title
            </h2>
            @*<h4>
                Book Your Hotel for the 17th Karnataka Short Course Swimming Championship - 2016, Mandya
            </h4>*@
        </div>
    </div>
    <div class="row">

        <div class="col-md-6">
            @using (Html.BeginForm("FormSubmit", "SubmitForm", FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-horizontal", id = "upload" }))
