@model IEnumerable<CLayer.Address>
@{
    Layout = null;   
}
    @if (Model != null)
{
    foreach (CLayer.Address usr2 in Model)
    {
            <text>
            Booking For:<br />
            First Name:  @usr2.Firstname<br />
            Address:@usr2.AddressText<br />
            City:@usr2.City <br />
            State: @usr2.StateName <br />
            Country:@usr2.Country<br />
            Phone:@usr2.Phone , @usr2.Mobile
            </text>
    }
}


