@model List<CLayer.OfflineBooking>
@{ Layout = null; }
<div class="col-lg-12">
    @if (Model.Count > 0)
    {
        @*<h4> List Of Properties for @Model[0].BusinessName</h4>*@

        <div class="height-popup">
            <table class="table">

                <thead>
                    <tr>
                        <th>User Name</th>
                        <th>User Type</th>
                        <th>Email</th>
                        
                        <th>Choose</th>
                        @*<th>Supplier Name</th>*@
                        <th style="display:none;">email</th>
                        <th style="display:none;">address</th>
                        <th style="display:none;">FirstName</th>
                        <th style="display:none;">Mobile</th>
                        <th style="display:none;">ccountry,state,city</th>
                        <th>

                        </th>

                    </tr>
                </thead>

                <tbody>
                    @foreach (CLayer.OfflineBooking item in Model)
                    {
                        <tr>
                            <td><span id="cusname_@item.CustomerId">@item.CustomerName</span></td>
                            <td>
                                @if (item.UserType != null)
                                {
                                    if (item.UserType > 0)
                                    {
                                       <text> @Enum.GetName(typeof(CLayer.Role.Roles), item.UserType) </text>
                                    }
                                }
                            </td>
                            <td>@item.CustomerEmail</td>
                            @*<td>@item.BusinessName</td>*@
                            <td style="display:none;"><span id="cusemail_@item.CustomerId">@item.CustomerEmail</span></td>
                            <td style="display:none;"><span id="cusAddress_@item.CustomerId">@item.CustomerAddress</span></td>
                            <td style="display:none;"><span id="cusmobile_@item.CustomerId">@item.CustomerMobile</span></td>


                            <td style="display:none;">
                                <span id="cuscountry_@item.CustomerId">@item.CustomerCountry</span>
                                <span id="cusstate_@item.CustomerId">@item.CustomerState</span>
                                <span id="cuscity_@item.CustomerId">@item.CustomerCity</span>
                                <span id="cuscityname_@item.CustomerId">@item.CustomerCityname</span>
                                <span id="UserTableCategory_@item.CustomerId">@item.CategoryType</span>
                                <span id="ZipCode_@item.CustomerId">@item.ZipCode</span>
                                <span id="NoInvoiceMail_@item.CustomerId">@item.NoInvoiceMail</span>

                            </td>
                            <td><a href="#" onclick="cusSelected(@item.CustomerId)">Choose</a></td>
                        </tr>
                    }

                </tbody>
            </table>
        </div>
    }
    else
    {
        if (ViewBag.ErrorMessage != null && ViewBag.ErrorMessage != "")
        {
            <div class="alert alert-info" role="alert">No matching data found.</div>
        }
        else
        {
            <div class="alert alert-info" role="alert">Please search for Customer</div>
        }
    }
</div>
