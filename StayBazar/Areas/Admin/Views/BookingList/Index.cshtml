@model StayBazar.Areas.Admin.Models.BookingListModel
@using Microsoft.AspNet.Identity
@{
    ViewBag.Title = "Booking List ";
}
<div class="container">
    <h2>@ViewBag.Title</h2>
    <div id="content">       
            <ul class="nav nav-tabs" id="myTab" data-tabs="tabs">
                <li class="active"><a href="#Recent" data-toggle="tab">Recent</a></li>
                <li><a href="#Recent" data-toggle="tab" onclick="FillPast()">Past</a></li>
            </ul>
        <div class="tab-content" id="my-tab-content">
            <div class="tab-pane active" id="Recent">
                    @if (ViewBag.Message != null)
                    {
                        <div class="alert alert-danger">
                            @ViewBag.Message
                        </div>
                    }
                    @{
                        Html.RenderPartial("~/Areas/Admin/Views/BookingList/_Recent.cshtml", Model);
                    }
            </div>
            <div class="tab-pane" id="Past">               
                    @if (ViewBag.Message != null)
                    {
                        <div class="alert alert-danger">
                            @ViewBag.Message.
                        </div>
                    }
                    @Html.Partial("~/Areas/Admin/Views/BookingList/_Past.cshtml", Model)
                </div>            
            </div>
        </div>
        </div>
<script type="text/javascript">
   function FillPast() {
     alert("Select id");
       $.post('@Url.Action("PastBookingData","BookingList")', function (data) {
               $("#Past").html(data);
       });              
   }
</script>