@{
    ViewBag.Title = "Import";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
    string cnts = BLayer.ExcelImport.GetTheCounts();
    string listSuemail = ViewBag.Emaillist;
    
    string[] nums = cnts.Split(',');
    int sup, prp, acc;
    sup = prp = acc = 0;
    if (nums.Length == 3)
    {
        sup = 0;
        if (nums[0] != null) { int.TryParse(nums[0], out sup); }
        if (nums[1] != null) { int.TryParse(nums[1], out prp); }
        if (nums[2] != null) { int.TryParse(nums[2], out acc); }
    }
}
<div class="container bdy_min_height">
    <h2>Import Details</h2>
    <div class="row">
        <div class="col-md-12">Data is ready for import</div>
        <div class="col-md-2 col-md-offset-1">
            Supplier Records:<br />
            Property Records:<br />
            Accommodation Records:
        </div>
        <div class="col-md-1">@sup<br />@prp<br />@acc</div>
        <div class="col-md-1 col-md-offset-3">
            @using (Html.BeginForm("DataImport", "ExcelImport", new { Area = "Admin", EmailList = listSuemail }))
            {
                <input type="submit" class="btn btn-default" value="Save Data" />

            }
        </div>
    </div>
</div>