                                                <div class="form-group">
                                                    <label for="CCCustomercommunications">Cc added for customer communications (Emails separated by comma, no spaces)</label>
                                                    @Html.TextBoxFor(m => m.CCCustomercommunications, new { @class = "form-control" })
                                                </div>
                                                @*add Cc For Suppliercommunications*@
                                                <div class="form-group">
                                                    <label for="CCSuppliercommunications">Cc added for supplier communications (Emails separated by comma, no spaces)</label>
