                                    <form method="post" action="@Url.Action("MailSave", "Settings", new { Area = "Admin" })">
                                        <div class="row">
                                            <div class="col-md-9">

                                                <div class="form-group">
                                                    @Html.LabelFor(m => m.MailServer)
                                                    @Html.TextBoxFor(m => m.MailServer, new { @class = "form-control" })
                                                </div>
                                                <div class="form-group">
                                                    @Html.LabelFor(m => m.Port)
                                                    @Html.TextBoxFor(m => m.Port, new { @class = "form-control" })
                                                </div>
                                                <div class="form-group">
                                                    @Html.LabelFor(m => m.MailSecurity)
                                                    @Html.DropDownListFor(m => m.MailSecurity, Model.SecurityListing, new { @class = "form-control" })
                                                </div>
                                                <div class="form-group">
                                                    <label for="QueryEmail">Query Email (No multiple emails)</label>
                                                    @Html.TextBoxFor(m => m.QueryEmail, new { @class = "form-control" })
                                                </div>
