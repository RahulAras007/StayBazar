@model StayBazar.Areas.Admin.Models.ReportDailyInventoryAndBookingModel
@{
    ViewBag.Title = "Daily Inventory And Booking Report";
    Layout = "~/Areas/Admin/Views/Shared/_reportLayout.cshtml";
}
<h2>@ViewBag.Title</h2>
<div id="content">
    <div class="row" id="Filter1">
        <div class="col-md-12">
            <div class="row margin10px" id="DetailsInventoryAndBooking">
                @Html.Partial("_Filtter", Model)
            </div>
        </div>
    </div>
    <div class="row" id="DailyInventoryAndBookingList">
        @if (Model != null)
        {
            @Html.Partial("_Report", Model.ReportList)
        }
    </div>
</div>
<script type="text/javascript">
    $(".container").removeClass("container").addClass("container-fluid");

</script>