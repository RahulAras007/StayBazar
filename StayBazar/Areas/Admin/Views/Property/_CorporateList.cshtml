@model List<CLayer.B2B>
@{ Layout = null; }    
<div class="col-lg-12">
    @if (Model.Count > 0)
{
    <div class="height-popup">
        <table class="table">
            <thead>
                <tr>
                    <th>Corporate Name</th>
                    <th>User Code</th>
                    <th>Choose</th>
                </tr>
            </thead>
            <tbody>
                @foreach(CLayer.B2B item in Model)
                {
                    <tr>
                        <td><span id="corpname_@item.B2BId" >@item.Name</span></td>
                        <td>@item.UserCode</td>
                        <td><a href="#" onclick="corpSelected(@item.B2BId)">Choose</a></td>
                    </tr>
                }

            </tbody>
        </table>
    </div>
}
else
{
        <div class="alert alert-info" role="alert">No data found!</div>
    }
</div>