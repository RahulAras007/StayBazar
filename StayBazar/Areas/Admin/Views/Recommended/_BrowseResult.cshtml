@model  List<CLayer.Property>
@{
    Layout = null;
}
<div class="col-md-12">
    @if (Model.Count > 0)
{
    <table class="table table-striped table-hover table-condensed">
        <thead>
            <tr>
                <td>Property</td>
                <td>Supplier</td>
                <td>Select</td>
            </tr>
        </thead>
        <tbody>
            @foreach (CLayer.Property pr in Model)
            {
                <tr>
                    <td><span id="prtit_@pr.PropertyId">@pr.Title</span></td>
                    <td>@pr.SupplierName</td>
                    <td><a href="#" onclick="choosePrp(@pr.PropertyId)"><span class="glyphicon glyphicon-ok"></span></a></td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
        <div class="alert alert-info">No results&hellip;</div>
    }
</div>